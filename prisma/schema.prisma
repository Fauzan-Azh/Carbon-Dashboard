datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ElectricityBill {
  id               Int       @id @default(autoincrement())
  panelName        String
  month            String
  kWhAmount        Int
  electricityCost  Float
  paymentStatus    String
  createdAt        DateTime  @default(now())
  updatedAt        DateTime  @updatedAt

  @@index([month])
}

